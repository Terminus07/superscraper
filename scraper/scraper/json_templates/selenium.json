{
    "name": "selenium",
    "start_urls": [
        "https://quotes.toscrape.com/page/1/"
    ],
    "driver_settings": {
        "driver_type": "Chrome",
        "options": {
            "experimental_options": [
                {
                    "name": "detach",
                    "value": true
                }
            ],
            "arguments": [
                "--headless",
                "--no-sandbox"
            ]
        },
        "capabilities": {
            "platform": "WINDOWS",
            "version": "10"
        }
    },
    "events": [
        {
            "function": "find_element",
            "args": {
                "by": "name",
                "value": "_user"
            },
            "output": "username"
        },
        {
            "function": "find_element",
            "args": {
                "by": "name",
                "value": "_pass"
            },
            "output": "password"
        },
        {
            "target": "username",
            "function": "send_keys",
            "args": "name"
        },
        {
            "target": "password",
            "function": "send_keys",
            "args": "red"
        },
        {
            "target": "password",
            "function": "send_keys",
            "args": "\ue007"
        },
        {
            "object_type": "Select",
            "object_input": "password",
            "args": {},
            "output": "select"
        },
        {
            "target": "select",
            "function": "select_by_index",
            "args": "1"
        }
    ],
    "request": {},
    "response": {},
    "custom_settings": {
        "LOG_ENABLED": false
    }
}