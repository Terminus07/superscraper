{
    "name": "selenium",
    "start_urls": [
        "https://quotes.toscrape.com/page/1/"
    ],
    "driver_settings": {
        "driver_type": "Chrome",
        "options": {
            "experimental": {
                "name": "detach",
                "value": true
            },
            "arguments": {}
        }
    },
    "events": [
        {
            "target": "driver",
            "function": "find_element",
            "args": {
                "by": "xpath",
                "value": "//title"
            },
            "output": "red"
        }
    ],
    "request": {},
    "response": {},
    "custom_settings": {
        "LOG_ENABLED": false
    }
}